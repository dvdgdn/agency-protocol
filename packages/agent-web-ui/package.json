{
  "name": "@agency-protocol/agent-web-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@agency-protocol/agent-registry": "^0.1.0",
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0"
  },
  "nx": {
    "name": "agent-web-ui",
    "targets": {
      "build": {
        "executor": "@nx/next:build",
        "outputs": ["{options.outputPath}"],
        "defaultConfiguration": "production",
        "options": {
          "outputPath": "dist/packages/agent-web-ui"
        },
        "configurations": {
          "development": {
            "outputPath": "packages/agent-web-ui"
          },
          "production": {}
        }
      },
      "serve": {
        "executor": "@nx/next:server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "agent-web-ui:build",
          "dev": true,
          "port": 4200
        },
        "configurations": {
          "development": {
            "buildTarget": "agent-web-ui:build:development",
            "dev": true
          },
          "production": {
            "buildTarget": "agent-web-ui:build:production",
            "dev": false
          }
        }
      }
    }
  }
}