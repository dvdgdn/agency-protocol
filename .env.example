# Supabase Database URLs
# Get these from your Supabase project's Settings > Database page

# For Prisma Client (with pg-bouncer for connection pooling)
DATABASE_URL="prisma://postgres.[YOUR-SUPABASE-ID].supabase.co:6543/postgres?pg-bouncer=true"

# For Prisma Migrate (direct connection)
DIRECT_URL="postgres://postgres:[YOUR-PASSWORD]@[YOUR-SUPABASE-ID].supabase.co:5432/postgres"

# API Configuration
API_PORT=3000

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000/api