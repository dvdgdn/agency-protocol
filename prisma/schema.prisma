generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model cas_store {
  cid        String    @id
  mime_type  String    @default("application/octet-stream")
  data       Bytes
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}
